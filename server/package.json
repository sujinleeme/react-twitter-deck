{
	"name": "twitter-auth-server",
	"version": "0.0.1",
	"description": "Server for React/Node Twitter auth",
	"main": "server.js",
	"scripts": {
		"server": "nodemon server.js",
		"start": "node server.js",
		"dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
		"client": "cd ../client && yarn start"
	},
	"dependencies": {
		"body-parser": "^1.19.0",
		"concurrently": "^5.0.0",
		"cookie-parser": "^1.4.4",
		"cookie-session": "^1.3.3",
		"cors": "^2.8.5",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"express-session": "^1.17.0",
		"passport": "^0.4.0",
		"passport-twitter": "^1.0.4",
		"request": "^2.88.0",
		"request-promise": "^4.2.4",
		"socket.io": "^2.3.0",
		"twit": "^2.2.11"
	},
	"engines": {
		"node": "^12.11.1",
		"yarn": "^1.19.1"
	},
	"license": "MIT",
	"devDependencies": {
		"nodemon": "^1.19.4"
	}
}